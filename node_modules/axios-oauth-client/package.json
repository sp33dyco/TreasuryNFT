{
  "name": "axios-oauth-client",
  "version": "1.5.0",
  "description": "OAuth 2.0 client utils for axios",
  "main": "dist/",
  "author": "Jonathon Hill <jhill9693@gmail.com>",
  "license": "MIT",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compwright/axios-oauth-client.git"
  },
  "keywords": [
    "oauth",
    "oauth2",
    "axios",
    "interceptor"
  ],
  "bugs": {
    "url": "https://github.com/compwright/axios-oauth-client/issues"
  },
  "homepage": "https://github.com/compwright/axios-oauth-client#readme",
  "engines": {
    "node": ">=10"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "nyc": {
    "cache": true,
    "cacheDir": ".nyc_cache",
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "axios-token-interceptor": "^0.2.0",
    "mocha": "*",
    "nyc": "*",
    "semistandard": "*"
  }
}
