{
  "name": "@notabene/pii-sdk",
  "description": "SDK to manage PII encryption",
  "homepage": "https://gitlab.com/notabene/open-source/pii-sdk",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/notabene/open-source/pii-sdk"
  },
  "author": "Notabene <developers@notabene.id>",
  "license": "MIT",
  "version": "1.16.2",
  "packageManager": "yarn@3.2.2",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "start": "node dist/index.js",
    "start:debug": "ts-node src/index.ts",
    "watch": "parcel watch",
    "build": "yarn build:clean && yarn build:compile",
    "build:clean": "rimraf dist/",
    "build:compile": "parcel build --no-source-maps",
    "lint": "eslint .",
    "test": "vitest run --no-color --run --reporter=verbose --no-api --no-threads --no-ui",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@parcel/core": "^2.8.3",
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^9.5.1",
    "@semantic-release/npm": "^9.0.2",
    "@types/axios-token-interceptor": "^0.2.4",
    "@types/lodash": "^4.14.194",
    "@types/node": "^16.18.26",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "organize-imports-cli": "^0.9.0",
    "parcel": "^2.8.3",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.2",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  },
  "lint-staged": {
    "*.ts": [
      "organize-imports-cli",
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@ethersproject/bytes": "5.7.0",
    "@noble/curves": "^1.1.0",
    "@stablelib/ed25519": "1.0.3",
    "axios": "^0.27.2",
    "axios-oauth-client": "^1.5.0",
    "axios-token-interceptor": "^0.2.0",
    "base64url": "3.0.1",
    "bs58": "5.0.0",
    "debug": "^4.3.4",
    "did-jwt": "6.11.6",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "multibase": "4.0.6",
    "multicodec": "3.2.1",
    "node-fetch": "^3.3.1",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0"
  }
}